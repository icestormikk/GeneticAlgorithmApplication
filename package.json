{
  "name": "gen-alg-app",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "main": "build/electron/index.js",
  "dependencies": {
    "@react-three/fiber": "^8.10.0",
    "@reduxjs/toolkit": "^1.9.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-force-graph-3d": "^1.21.17",
    "react-icons": "^4.7.1",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "three": "^0.148.0",
    "three-spritetext": "^1.6.5",
    "typescript": "^4.9.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
    "electron:build": "npm build && tsc -p electron && electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^22.0.2",
    "electron-builder": "^23.6.0",
    "eslint": "^8.31.0",
    "eslint-config-google": "^0.14.0",
    "tailwindcss": "^3.2.4",
    "wait-on": "^7.0.1"
  },
  "build": {
    "extends": null,
    "files": [
      "./build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
